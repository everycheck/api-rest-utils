{
    "name": "everycheck/api-rest-utils",
    "type": "library",
    "license": "MIT",
    "require": {
        "symfony/symfony": "3.4.*"
    },
    "require-dev": {
        "php": ">=5.5.9",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "jms/serializer-bundle": "^1.0 | ^2.0"
    },
    "autoload": {
        "psr-4": {
            "EveryCheck\\ApiRest\\Utils\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EveryCheck\\ApiRest\\Utils\\Tests\\": "Tests"
        }
    }
}